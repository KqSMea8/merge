<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@id/home_history_container" android:background="#ffffffff" android:layout_width="fill_parent" android:layout_height="@dimen/home_history_view_height"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ImageView android:id="@id/home_history_time_icon" android:layout_width="@dimen/home_personal_movie_30px" android:layout_height="@dimen/home_personal_movie_30px" android:layout_marginLeft="@dimen/home_personal_movie_20px" android:layout_marginRight="@dimen/home_personal_movie_10px" android:src="@drawable/home_history_view_left_icon" android:scaleType="centerCrop" android:layout_alignParentLeft="true" android:layout_centerVertical="true" />
    <TextView android:textSize="@dimen/home_personal_movie_28px" android:textColor="#ff2f95ff" android:ellipsize="end" android:gravity="center_vertical" android:id="@id/home_history_txt" android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_marginRight="@dimen/home_personal_movie_20px" android:singleLine="true" android:layout_toLeftOf="@id/home_history_right_arrow" android:layout_toRightOf="@id/home_history_time_icon" />
    <ImageView android:id="@id/home_history_right_arrow" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/home_personal_movie_36px" android:src="@drawable/home_history_right_arrow" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
</RelativeLayout>